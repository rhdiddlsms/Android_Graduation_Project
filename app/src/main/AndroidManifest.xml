<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gmchan.example.registeration">

    <!-- 인터넷 연결 허용상태 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="동수도"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name="gmchan.example.registeration.Splashscreen"
            android:label="동수도">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="gmchan.example.registeration.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name="gmchan.example.registeration.RegisterActivity" />
        <activity android:name="gmchan.example.registeration.MainActivity"></activity>
        <activity android:name="gmchan.example.registeration.Pop"
            android:theme="@style/AppTheme.Pop"></activity>


    </application>

</manifest>